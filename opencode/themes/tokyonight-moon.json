{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg": "#222436",
    "bg_dark": "#1e2030",
    "bg_highlight": "#2f334d",
    "terminal_black": "#1b1d2b",
    "fg": "#c8d3f5",
    "fg_dark": "#828bb8",
    "fg_gutter": "#3b4261",
    "dark5": "#737aa2",
    "blue": "#82aaff",
    "cyan": "#86e1fc",
